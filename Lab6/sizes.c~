//sizes.c

#include <stdio.h>

void main(){
  int i;
  long int l;
  float f;
  double d;
  short s;
  char c;
  unsigned long long int num = sizeof(i);
  printf("Integer: %llu", num);
  num = sizeof(l);
  printf("Long Int: %llu", num);
  num = sizeof(f);
  printf("Float: %llu", num);
  num = sizeof(d);
  printf("Double: %llu", num);
  num = sizeof(s);
  printf("Short: %llu", num);
  num = sizeof(c);
  printf("Character: %llu", num);
}
